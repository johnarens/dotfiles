{
   "background": {
      "persistent": false,
      "scripts": [ "jquery-2.1.1.min.js", "background.js" ]
   },
   "content_scripts": [ {
      "css": [ "gqueuesGmailExt-min.css" ],
      "js": [ "jquery-2.1.1.min.js", "jquery-ui-1.11.2.min.js", "gqueuesGmailExt-min.js" ],
      "matches": [ "https://mail.google.com/*" ],
      "run_at": "document_end"
   }, {
      "js": [ "jquery-2.1.1.min.js", "loginContentScript.js" ],
      "matches": [ "https://www.gqueues.com/gmailext?*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "description": "Turn emails into actionable tasks with GQueues - the leading Google-integrated task management system for people and teams.",
   "icons": {
      "128": "gqIcon128.png",
      "16": "gqIcon16.png",
      "48": "gqIcon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "GQueues for Gmail",
   "permissions": [ "storage", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.9",
   "web_accessible_resources": [ "images/*" ]
}
